{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "jsxSingleQuote": false,
  "htmlWhitespaceSensitivity": "ignore",
  "trailingComma": "none",
  "useTabs": false,
  "rangeStart": 0,
  "proseWrap": "always",
  "endOfLine": "lf",
  "arrowParens": "avoid"
}
